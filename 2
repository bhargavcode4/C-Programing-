// if else practice
#include<stdio.h>
int main(){
    int age;
    printf("Enter your age:");
    scanf("%d",&age);
    printf("%d is ur age \n",age);
    if(age>=18){
        printf("u can vote");
    }
    else if(age>10)
    {
        printf("U r b/w 10 to 18 and u vote fro kids");
    }
    else if(age>3)
    {
        printf(" u r age is grter than 3 so u vote for baby");
    }
    else{
        printf("U cant vote");
    }
    return 0;
}
